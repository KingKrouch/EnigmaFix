{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build EnigmaFix (Debug)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "-B",
                "${workspaceFolder}/Intermediate/Debug",
                "-DCMAKE_BUILD_TYPE=Debug",
                "${workspaceFolder}"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        },
        {
            "label": "Build EnigmaFix (Release)",
            "type": "shell",
            "command": "cmake",
            "args": [
                "-B",
                "${workspaceFolder}/Intermediate/Release",
                "-DCMAKE_BUILD_TYPE=Release",
                "${workspaceFolder}"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        }
    ]
}